-> Ak index ko pakad ke aur minimum nikal ke swap karte rho..

void SelectionSort(int arr[],int n)
{
int i, j, min_index;
 for(int i =0;i<n-1;i++)
 {
 min_index =i;
 for(int j =i+1;j<n;j++)
 {
  if(arr[j]<arr[min_index]
    }
        // Swap the found minimum element 
        // with the first element 
        if (min_idx!=i)
            swap(&arr[min_idx], &arr[i]);
    }
} 
